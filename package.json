{
  "name": "go-htmx-sandbox",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prod": "tailwind && build",
    "dev": "npm-run-all --parallel tailwind:watch build:watch",
    "build:watch": "pnpm run build --watch",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "tailwind:watch": "pnpm run tailwind --watch",
    "tailwind": "npx tailwindcss -i ./css/style.css -o ./dist/main.css"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "rollup": "^4.3.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.8"
  }
}
